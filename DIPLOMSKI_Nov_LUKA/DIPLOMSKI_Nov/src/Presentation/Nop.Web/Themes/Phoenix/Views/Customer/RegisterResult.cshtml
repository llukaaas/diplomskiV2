@model RegisterResultModel
@{
    Layout = "_ColumnsOne";

    //title
    NopHtml.AddTitleParts(T("PageTitle.Register").Text);
    //page class
    NopHtml.AppendPageCssClassParts("html-registration-result-page");
}
<div class="page registration-result-page section-padding">
    <div class="section-width">
        <div class="section-head">
            <h1>@T("Account.Register")</h1>
        </div>
        <div class="page-body">
            <div class="result">
                @Model.Result
            </div>
            <div class="buttons">
                <a href="@Model.ReturnUrl" class="button-1 register-continue-button primary-btn">@T("Common.Continue")</a>
            </div>
        </div>
    </div>
</div>